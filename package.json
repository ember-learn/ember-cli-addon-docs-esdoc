{
  "name": "ember-cli-addon-docs-esdoc",
  "version": "0.2.3",
  "description": "ESDoc plugin for ember-cli-addon-docs",
  "keywords": [
    "ember-addon",
    "ember-cli-addon-docs-plugin"
  ],
  "repository": "https://github.com/ember-learn/ember-cli-addon-docs-esdoc",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "mocha node-tests",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "broccoli-caching-writer": "^3.0.3",
    "ember-cli-babel": "^7.26.3",
    "ember-cli-htmlbars": "^5.7.1",
    "esdoc": "pzuraq/esdoc#015a342",
    "esdoc-accessor-plugin": "^1.0.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-typescript-plugin": "^1.0.1",
    "fs-extra": "^5.0.0",
    "json-api-serializer": "^1.11.0",
    "lodash": "^4.17.21",
    "resolve": "^1.20.0",
    "tmp": "^0.2.1",
    "walk-sync": "^2.2.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.5",
    "@embroider/test-setup": "^0.37.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.3.4",
    "ember-auto-import": "^1.11.2",
    "ember-cli": "~3.26.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.1",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.26.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.2.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-ember": "^10.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "loader.js": "^4.7.0",
    "mocha": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit": "^2.14.1",
    "qunit-dom": "^1.6.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "12.22.1",
    "yarn": "1.22.10"
  }
}
